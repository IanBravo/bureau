<h1><%= @resource_model.model.to_s.pluralize %></h1>

<table>
  <thead>
    <tr>
      <% @resource_model.collection_attributes.each do |attribute| %>
      <th><%= attribute.to_s.titleize %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
      <% @resources.each do |resource| %>
      <tr>
        <% @resource_model.collection_attributes.each do |attribute| %>
        <td><%= resource.send attribute %></td>
        <% end %>
      </tr>
      <% end %>
  </tbody>
</table>